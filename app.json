{
  "expo": {
    "name": "Vint Street",
    "slug": "vintstreet-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vintstreetapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.vintstreet.app",
      "buildNumber": "1",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Vint Street uses your camera to take photos of items you want to sell and to update your profile picture.",
        "NSMicrophoneUsageDescription": "Vint Street uses your microphone during live streaming sessions to communicate with buyers in real-time.",
        "NSPhotoLibraryUsageDescription": "Vint Street uses your photo library to select existing photos of items you want to sell. For example, you can choose photos of vintage clothing or accessories from your library to create a listing."
      }
    },
    "android": {
      "package": "com.vintstreet.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#000000",
          "sounds": []
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "urlScheme": "vintstreetapp"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "59fe92e2-a0e3-4b82-847c-ac926bd486f9"
      }
    }
  }
}
